<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:tools="http://schemas.android.com/tools"    android:layout_width="match_parent"    android:layout_height="match_parent"    tools:context="com.etrack.myapplication.fragment.DeleteBranchFragment">    <ScrollView        android:layout_width="match_parent"        android:layout_height="match_parent">        <LinearLayout            android:layout_width="match_parent"            android:layout_height="match_parent"            android:gravity="center"            android:orientation="vertical">            <LinearLayout                android:layout_width="match_parent"                android:layout_height="match_parent"                android:orientation="vertical">                <android.support.design.widget.TextInputLayout                    android:layout_width="match_parent"                    android:layout_height="wrap_content"                    android:layout_margin="10dp"                    android:layout_marginTop="0dp">                    <EditText                        android:id="@+id/et_itembarcode"                        android:layout_width="match_parent"                        android:layout_height="wrap_content"                        android:hint="@string/item_barcode"                        android:inputType="text"                        android:textColor="#000000"                        android:visibility="gone" />                </android.support.design.widget.TextInputLayout>                <Spinner                    android:id="@+id/spItemList"                    android:layout_width="match_parent"                    android:layout_height="wrap_content"                    android:layout_margin="@dimen/spacing_smaller_8"                    android:minHeight="70dp"                    android:prompt="@string/not_valid" />                <Button                    android:id="@+id/btn_serch_item"                    android:layout_width="fill_parent"                    android:layout_height="wrap_content"                    android:layout_margin="10dp"                    android:layout_marginTop="0dp"                    android:background="@color/colorAccent"                    android:text="@string/search_item"                    android:textColor="@android:color/white"                    android:visibility="gone" />                <android.support.design.widget.TextInputLayout                    android:layout_width="match_parent"                    android:layout_height="wrap_content"                    android:layout_margin="10dp"                    android:layout_marginTop="0dp">                    <EditText                        android:id="@+id/et_itemname"                        android:layout_width="match_parent"                        android:layout_height="wrap_content"                        android:hint="@string/item_name"                        android:textColor="#000000" />                </android.support.design.widget.TextInputLayout>                <android.support.design.widget.TextInputLayout                    android:layout_width="match_parent"                    android:layout_height="wrap_content"                    android:layout_margin="10dp"                    android:layout_marginTop="0dp">                    <EditText                        android:id="@+id/et_itemname_arabic"                        android:layout_width="match_parent"                        android:layout_height="wrap_content"                        android:hint="@string/arabic_name"                        android:textColor="#000000" />                </android.support.design.widget.TextInputLayout>                <android.support.design.widget.TextInputLayout                    android:layout_width="match_parent"                    android:layout_height="wrap_content"                    android:layout_margin="10dp"                    android:layout_marginTop="0dp">                    <EditText                        android:id="@+id/et_item_catogorey"                        android:layout_width="match_parent"                        android:layout_height="wrap_content"                        android:hint="@string/catogorey"                        android:textColor="#000000" />                </android.support.design.widget.TextInputLayout>                <android.support.design.widget.TextInputLayout                    android:layout_width="match_parent"                    android:layout_height="wrap_content"                    android:layout_margin="10dp"                    android:layout_marginTop="0dp">                    <EditText                        android:id="@+id/et_openning_qty"                        android:layout_width="match_parent"                        android:layout_height="wrap_content"                        android:hint="@string/openning_qty"                        android:inputType="number"                        android:textColor="#000000" />                </android.support.design.widget.TextInputLayout>                <android.support.design.widget.TextInputLayout                    android:layout_width="match_parent"                    android:layout_height="wrap_content"                    android:layout_margin="10dp"                    android:layout_marginTop="0dp">                    <EditText                        android:id="@+id/et_cost_price"                        android:layout_width="match_parent"                        android:layout_height="wrap_content"                        android:hint="@string/cost_price"                        android:inputType="numberDecimal"                        android:textColor="#000000" />                </android.support.design.widget.TextInputLayout>                <android.support.design.widget.TextInputLayout                    android:layout_width="match_parent"                    android:layout_height="wrap_content"                    android:layout_margin="10dp"                    android:layout_marginTop="0dp">                    <EditText                        android:id="@+id/et_sale_price"                        android:layout_width="match_parent"                        android:layout_height="wrap_content"                        android:hint="@string/sale_price"                        android:inputType="numberDecimal"                        android:textColor="#000000" />                </android.support.design.widget.TextInputLayout>                <android.support.design.widget.TextInputLayout                    android:layout_width="match_parent"                    android:layout_height="wrap_content"                    android:layout_margin="10dp"                    android:layout_marginTop="0dp">                    <EditText                        android:id="@+id/et_net_price"                        android:layout_width="match_parent"                        android:layout_height="wrap_content"                        android:enabled="false"                        android:hint="@string/net_rate"                        android:inputType="numberDecimal"                        android:textColor="#000000" />                </android.support.design.widget.TextInputLayout>                <android.support.design.widget.TextInputLayout                    android:layout_width="match_parent"                    android:layout_height="wrap_content"                    android:layout_margin="10dp"                    android:layout_marginTop="0dp">                    <EditText                        android:id="@+id/et_vat"                        android:layout_width="match_parent"                        android:layout_height="wrap_content"                        android:hint="@string/vat"                        android:inputType="numberDecimal"                        android:textColor="#000000" />                </android.support.design.widget.TextInputLayout>            </LinearLayout>            <Button                android:id="@+id/btn_deleteItem"                android:layout_width="fill_parent"                android:layout_height="wrap_content"                android:layout_margin="@dimen/spacing_smaller_8"                android:background="@color/colorAccent"                android:text="@string/delete_item"                android:textColor="@android:color/white" />        </LinearLayout>    </ScrollView></FrameLayout>